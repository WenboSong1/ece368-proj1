long *Load_File (char *Filename, int *Size)
{
	FILE * file;
	file = fopen( "Filename", "r");
	if(file)
	{
		fscanf(file, "%s", *Size);
		for(int i = 0, i < Size, i++)
		{
			long *array[Size];
			fscanf(file, "%s", array[i];
		}		
		fclose(file);
	}
	return array;
}
int Save_File (char *Filename, long *Array, int Size)
{
	FILE * file;
	file = fopen("Filename", "w");
	if(file != NULL)
	{
		scanf("$d\n", Size);
		for(int i = 0, i < Size, i++)
		{
			scanf("%d\n", Array[i];
		}
		fclose(file);
		return Size;
	}
}
